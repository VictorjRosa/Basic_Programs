<Window x:Class="Tarea_4___Udemy.LoginDialogBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tarea_4___Udemy"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner"
        Title="Login">
    <StackPanel>
        <Label>Nombre de usuario:</Label>
        <TextBox ></TextBox>
        <Label>Contraseña:</Label>
        <PasswordBox></PasswordBox>
        <Button Name="AceptarButton" Click="Button_Click" IsDefault="True">Aceptar</Button>
        <Button Name="NoTengoButton" Click="Button_Click_1" IsCancel="True">No tengo</Button>
    </StackPanel>
</Window>
